{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "price": {
      "unique": false,
      "required": true,
      "type": "integer"
    },
    "weight": {
      "type": "integer",
      "required": true
    },
    "old_price": {
      "type": "integer",
      "required": true
    }
  }
}
